{
    "name": "nails/module-custom-forms",
    "keywords": [
        "Nails",
        "Shed Collective",
        "CodeIgniter",
        "Custom Forms Module"
    ],
    "homepage": "http://nailsapp.co.uk/modules/custom-forms",
    "description": "This is the \"Custom Forms\" module for Nails.",
    "license": "MIT",
    "authors": [
        {
            "name": "Pablo de la Pena",
            "email": "p@nailsapp.co.uk",
            "homepage": "http://nailsapp.co.uk",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "hello@nailsapp.co.uk"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "nails/common": "dev-feature/pre-new-admin",
        "nails/module-admin": "dev-feature/pre-new-admin",
        "nails/module-captcha": "dev-feature/pre-new-admin",
        "nails/module-email": "dev-feature/pre-new-admin",
        "nails/module-form-builder": "dev-feature/pre-new-admin"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*",
        "phpstan/phpstan": "1.*"
    },
    "scripts": {
        "test": "./vendor/bin/phpunit",
        "analyse": "./vendor/bin/phpstan analyse -c .phpstan/config.neon"
    },
    "autoload": {
        "psr-4": {
            "Nails\\CustomForms\\": "src/"
        }
    },
    "extra": {
        "nails": {
            "moduleName": "forms",
            "type": "module",
            "namespace": "Nails\\CustomForms\\",
            "data": {
                "nails/module-admin": {
                    "autoload": {
                        "assets": {
                            "js": [
                                "admin.min.js"
                            ],
                            "css": [
                                "admin.min.css"
                            ]
                        }
                    }
                }
            }
        }
    }
}
